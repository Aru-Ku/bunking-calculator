{
	"expo": {
		"name": "bunking-calculator",
		"slug": "bunking-calculator",
		"platforms": ["ios", "android", "web"],
		"packagerOpts": {
			"config": "metro.config.js",
			"sourceExts": ["js", "jsx", "svg", "ts", "tsx"]
		},
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon1.png",
		"splash": {
			"image": "./assets/splash1.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "com.mastrero.bunking.calculator",
			"permissions": []
		},
		"androidStatusBar": {
			"hidden": true
		},
		"notification": {
			"color": "#ffffff"
		}
	}
}
